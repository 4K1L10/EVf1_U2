@model IEnumerable<Developers.Models.Recepcionequipo>

@{
    ViewData["Title"] = "Recepcione de equipos";
}

<h1>Recepcione de equipos</h1>

<p>
    <a asp-action="Create" class="btn btn-outline-info">Nueva Recepcione de equipo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoPc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Accesorio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MarcaPc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoledoPc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nserie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CanpacidadRam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoAlmacenamiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoGpu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Grafico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdClienteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdServicioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoPc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Accesorio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MarcaPc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MoledoPc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nserie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CanpacidadRam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoAlmacenamiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoGpu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Grafico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdClienteNavigation.IdCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdServicioNavigation.IdServicio)
            </td>
            <td>
                    <a asp-action="Edit" asp-route-id="@item.IdRe" class="btn btn-outline-primary">Editar</a> |
                    <a asp-action="Details" asp-route-id="@item.IdRe" class="btn btn-outline-secondary">Ver</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdRe" class="btn btn-outline-warning">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
